		"rollup-plugin-typescript2": "^0.17.0",
		"rollup-plugin-url": "^3.0.1",
		"typescript": "^3.9.6"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"react",
		"typescript",
		"javascript",
		"commonjs",
		"i18n",
		"intl",
		"react-native",
		"Internationalization"
