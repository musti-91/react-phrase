		"predeploy": "cd example && yarn install && yarn run build",
		"deploy": "gh-pages -d example/build"
	},
	"dependencies": {
		"node-polyglot": "^2.4.0"
	},
	"peerDependencies": {
		"node-polyglot": "^2.4.0",
		"prop-types": "^15.5.4",
