		"react-dom": "^15.0.0 || ^16.0.0"
	},
	"devDependencies": {
		"@svgr/rollup": "^2.4.1",
		"@types/jest": "^29.0.0",
		"@types/node-polyglot": "^0.4.34",
		"@types/react": "^17.0.15",
		"@types/react-dom": "^16.0.5",
		"babel-core": "^6.26.3",
		"babel-runtime": "^6.26.0",
		"cross-env": "^5.1.4",
		"gh-pages": "^3.2.3",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"react-scripts-ts": "^4.0.8",
